        <InterfaceGroup>
          <Filter>
            <LanguageId RequiredValue="guidIrisLanguageId"/>
          </Filter>
          <Interface Name="IDkmLanguageFrameDecoder"/>
        </InterfaceGroup>
      </Implements>
    </Class>

  </ManagedComponent>

</Configuration>
